[run]
branch = True
source =
    src

[report]
show_missing = True
skip_covered = False
skip_empty = False
precision = 2
fail_under = 0
omit =
    */migrations/*
    */settings/*
    */tests/*
    venv/*
    **/__init__.py
exclude_lines =
    pragma: no cover
    if __name__ == "__main__":

[html]
directory = htmlcov
